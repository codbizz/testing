<%- include('./../header')-%>

    <%- include('./../navbar',{user: user})-%>
        <div class="content-wrapper">
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>Test Series Details</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="/admin">Home</a></li>
                                <li class="breadcrumb-item active">Test Series Details</li>
                            </ol>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>

            <!-- Main content -->
            <section class="content">

                <%- include('./../alert') %>

                    <div class="row">
                        <div class="col-md-6 offset-md-3">
                            <table class="table table-hover text-nowrap">

                                <tbody>
                                    <tr>
                                        <th style="width:70px">ID</th>
                                        <td><%- testDetails._id ?? "" %></td>
                                    </tr>
                                    <tr>
                                        <th style="width:70px">Title in English</th>
                                        <td><%- testDetails.title.EN ?? "" %></td>
                                    </tr>
                                    <tr>
                                        <th style="width:70px">Title in Hindi</th>
                                        <td><%- testDetails.title.HI ?? "" %></td>
                                    </tr>
                                    <tr>
                                        <th style="width:70px">Number Of Questions</th>
                                        <td><%- testDetails.question_ids.length %></td>
                                    </tr>
                                    <tr>

                                        <th style="width:70px">Set Title</th>
                                        <td> <a href="/admin/set-detail/<%- testDetails.set_id._id %>"><%-
                                                    testDetails.set_id.title.EN %></a></td>
                                    </tr>





                                </tbody>
                            </table>
                            <div class="row">
                                <a href="/admin/update-test/<%- testDetails._id %>"> <button
                                        class="btn-primary">Update</button></a>

                                <a href="/admin/create-question?testId=<%-testDetails._id  %>"> <button
                                        class="btn-primary">Add New Question</button></a>

                                <a href="/admin/add-existing-question/<%-testDetails._id  %>"> <button
                                        class="btn-primary">Add Existing Question</button></a>
                            </div>

                        </div>


                    </div>

            </section>
            <%- include('./questions_table',{questionLists:testDetails.question_ids,subjectStrings}) %>
        </div>
        <%- include('./../footer')-%>